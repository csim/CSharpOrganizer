{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch CSharply.exe",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/artifacts/bin/CSharply/debug/CSharply.dll",
      "args": [
        "organize",
        "${workspaceFolder}/.scratch/test.cs",
        "--simulate",
      ],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
